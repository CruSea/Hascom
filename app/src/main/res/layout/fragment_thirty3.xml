<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:paddingTop="10dp"

    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/catholic_ena_christina_liyunet"
        android:textSize="20dp"
        android:textStyle="bold"
        android:background="#ffffff"
        android:textColor="#e46315"  />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        tools:context="gcme.hascom.hiliwinawFragments.FragmentSix">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tvThirty3"
            android:text="@string/catholic_ena_christina_liyunet_detail"
           android:textSize="18dp"
            android:textColor="#000000"  android:textIsSelectable="true"/>


